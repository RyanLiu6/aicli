{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "*",
      "Bash",
      "Read",
      "Write",
      "Edit",
      "Glob",
      "Grep",
      "Task",
      "TodoWrite",
      "WebFetch",
      "WebSearch",
      "NotebookEdit",
      "NotebookRead",
      "AskUserQuestion",
      "Skill",
      "SlashCommand",
      "BashOutput",
      "KillShell",
      "mcp__playwright__*",
      "mcp__chrome-devtools__*"
    ],
    "deny": [],
    "ask": [
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(rm:*)"
    ],
    "defaultMode": "acceptEdits"
  },
  "enabledPlugins": {
    "feature-dev@claude-code-plugins": true,
    "frontend-design@claude-code-plugins": true,
    "pr-review-toolkit@claude-code-plugins": true
  },
  "alwaysThinkingEnabled": true
}
